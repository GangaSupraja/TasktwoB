package tasktwo;

import java.util.Scanner;

public class TasktwoB {

	public static void main(String[] args) {
		
		int digit, count=0;
		Scanner sc=new Scanner(System.in);
		int num=sc.nextInt();
		while(num>0) {
			digit=num%10;
			num=num/10;
			if(digit==4) {
				count++;
			}
			
		}
		System.out.println(count);

	}

}
